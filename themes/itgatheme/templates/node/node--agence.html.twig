    'villeAgence': node.field_ville.value,
    'telAgence': node.field_tel.value,
    'faxAgence': node.field_fax.value,
    'emailAgence': node.field_email.value,
}
%}

{% include 'themes/itgatheme/templates/node/agence-tpl.html.twig' with vars %}
